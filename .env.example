# Exemplo de arquivo .env para produção
# Copie para .env e preencha com seus valores reais

# Supabase Configuration
SUPABASE_URL=https://seu-projeto.supabase.co
SUPABASE_ANON_KEY=sua-anon-key-aqui
SUPABASE_SERVICE_ROLE_KEY=sua-service-role-key-aqui

# Session Secret (gere com: openssl rand -base64 32)
SESSION_SECRET=substitua-por-um-secret-aleatorio-seguro

# Environment
NODE_ENV=production
PORT=5000

# Paths
XML_DEST_PATH=/app/xmls

# Development only - DESABILITE EM PRODUÇÃO
ALLOW_SEFAZ_SIMULATION=false
